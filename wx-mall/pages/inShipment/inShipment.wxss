.all{
  width: 750rpx;
  height: 100vh;
  margin: 0;
  padding: 0;
}
.all .Image{
  position: relative;
  width: 100%;
}
.all .Image .centerPic{
  width: 100%;
  position: relative;
  top: 90%;
  display: flex;
  justify-content: center;
}
.all .centerPic .Txt{
  width: 100%;
  position: absolute;
  top: 125%;
  text-align: center;
}
.all .Txt .machineError{
  width: fit-content;
  margin: 0 auto 56rpx;
  color: #FF9700;
  font-size: 40rpx;
}
.all .centerPic .Txt .mark{
  color: #333;
  font-size: 26rpx;
  width: fit-content;
  margin: 0 auto 56rpx;
}
.all .BtnPosition{
  position: fixed;
  bottom: 11%;
  width: 100%;
}
.all .BtnPosition .button{
  background-color: #082F40;
  color: #ffffff;
  font-size: 32rpx;
  border-radius: 25px;
  width: 610rpx;
}