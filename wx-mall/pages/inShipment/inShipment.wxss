/* 自定义导航栏 */
/* 顶部 */
.top {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 99;
}

.top .status {
  width: 100%;
}

.top .comment {
  width: 100%;
}

.top .comment .FlexBox {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  margin-bottom: 12rpx;
}

.top .comment .FlexBox .backButton {
  display: flex;
  justify-content: start;
  align-items: center;
}

.backButtonImage {
  width: 40rpx;
  height: 40rpx;
}

.mainTitle {
  color: #fff;
  font-size: 17px;
}

.blank {
  width: 40rpx;
  height: 40rpx;
}
