.pop .content {
  position: absolute;
  left: 0;
  top: 23vh;
  width: 100%;
  height: 77vh;
  margin: 0;
  z-index: 1000;
}

.pop .content .message {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
}

.pop .content .message .popTitle {
  margin: 30rpx 30rpx 0;
}

.pop .content .message .popTitle .purchaseCount {
  color: #333333;
  font-size: 32rpx;
  height: 44rpx;
  line-height: 44rpx;
}

.pop .content .message .popTitle .leftToBuy {
  color: #FD5959;
  font-size: 24rpx;
  vertical-align: bottom;
  height: 44rpx;
  line-height: 44rpx;
}
.pop .content .message .popTitle .noShow{
  width: 34rpx;
  height: 34rpx;
  float: right;
}
.pop .content .message .numberBox{
  display: flex;
    justify-content: center;
    margin: 34rpx 0 0 0;
}
.pop .content .message .line{
  margin: 40rpx 30rpx 0;
  background-color: #CCCCCC;
  height: 2rpx;
}
.pop .content .message .chooseTxt{
  margin: 34rpx 0 0 30rpx;
  font-size: 32rpx;
  color: #333;
  font-weight:bold;
}
.pop .content .message .chooseVolume{
  margin: 36rpx 30rpx 0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.pop .content .message .chooseVolume .left{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.pop .content .message .chooseVolume .left .img{
  width: 36rpx;
  height: 36rpx;
}
.pop .content .message .chooseVolume .left .txt{
  font-size: 30rpx;
  color: #333;
  margin-left: 26rpx;
}
.pop .content .message .chooseVolume .right{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.pop .content .message .chooseVolume .right .hasVolume{
  color: #ffffff;
  font-size: 24rpx;
  background-color: #FD5959;
  padding: 2rpx 18rpx;
  margin-right: 30rpx;
  border-radius: 8rpx;
}
.pop .content .message .chooseVolume .right .hasChoose{
  font-size: 30rpx;
  color: #FD5959;
  margin-right: 30rpx;
}
.pop .content .message .chooseVolume .right .go{
  width: 16rpx;
  height: 36rpx;
}
.pop .content .message .payWayTxt{
  margin: 34rpx 0 0 30rpx;
  font-size: 32rpx;
  color: #333;
  font-weight:bold;
}
.pop .content .message .payBalance{
  margin: 44rpx 0 0 30rpx;
  display: flex;
    align-items: center;
}
.pop .content .message .payBalance .img{
  width: 32rpx;
  height: 32rpx;
  margin-right: 30rpx;
}
.pop .content .message .payBalance .balanceTxt{
  font-size: 30rpx;
  color: #333;
}
.pop .content .message .payBalance .leftOfBalance{
  font-size: 25rpx;
  color: #333;
  margin-left: 10rpx;
}
.pop .content .message .payBalance .toDeduction{
  font-size: 30rpx;
  color: #FD5959;
  margin-left: auto;
    margin-right: 30rpx;
}
.pop .content .message .weChatPay{
  margin: 52rpx 0 0 30rpx;
  display: flex;
    align-items: center;
}
.pop .content .message .weChatPay .img{
  width: 34rpx;
  height: 30rpx;
  margin-right: 30rpx;
}
.pop .content .message .weChatPay .txt{
  font-size: 30rpx;
  color: #333;
}
.pop .content .message .realPay{
  margin: 56rpx 0 34rpx;
}
.pop .content .message .realPay .money{
  width: fit-content;
  margin: 0 auto;
  font-size: 80rpx;
  color: #333;
}
.pop .content .message .realPay .txt{
  width: fit-content;
  margin: 14rpx auto 0;
  font-size: 20rpx;
  color: #333;
}
