.top {
  width: 690rpx;
  margin: 60rpx auto 0;
}

.machine_detail_img {
  height: 340rpx;
  width: 690rpx;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
}

.machine_detail {
  background-color: white;
  border-radius: 0 0 10rpx 10rpx;
  box-shadow: 0 2rpx 20rpx rgba(98, 98, 98, 0.16);
}

.machine_detail_con {
  padding-top: 36rpx;
}

.machine_detail_top {
  /* padding-top: 36rpx; */
  height: 130rpx;
  width: 100%;
}

.machine_image {
  float: left;
  vertical-align: top;
  width: 130rpx;
  height: 130rpx;
  margin-left: 30rpx;
  border-radius: 6rpx;
  overflow: hidden;
}

.machine_name {
  vertical-align: top;
  height: 120rpx;
  width: 350rpx;
  padding: 40rpx 0 0;
  font-size: 30rpx;
  margin-left: 15rpx;
  font-weight: bold;
  color: #141616;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-break: break-all;
    float: left;
}

.machine_scan {
  width: 100rpx;
  height: 130rpx;
  margin-right: 30rpx;
  float: right;
  background-color: #F8F8F8;
  text-align: center;
  border-radius: 4rpx;
}

.machine_scan_image {
  width: 50rpx;
  height: 50rpx;
  margin: 20rpx auto;
}

.machine_scan_text {
  color: #141616;
  width: 100rpx;
  font-size: 12px;
  font-weight: 400;
  float: right;
}

.machine_error {
  display: inline-block;
  width: 135rpx;
  height: 130rpx;
  text-align: center;
  font-size: 12px;
  line-height: 130rpx;
  margin-left: 10rpx;
  color: #FF0000;
}

/* 工作时间、工作地点、导航按钮 */
.machine_detail_center {
  /* height: 100rpx; */
  width: 100%;
  margin-top: 10rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.machine_detail_center_left {
  width: 520rpx;
  margin-left: 28rpx;
}

.machine_worktime {
  display: flex;
  align-items: center;
  margin-top: 5rpx;
  vertical-align: top;
  line-height: 40rpx;
  width: 500rpx;
  font-size: 20rpx;
  color: #999999;
  white-space: nowrap;
  overflow: hidden;
  margin-bottom: 18rpx;
}

.machine_worktime image {
  display: inline-block;
  height: 22rpx;
  width: 22rpx;
  margin-right: 8rpx;
  margin-top: 4rpx;
}

.machine_worktime view {
  display: inline-block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 20rpx;
  height: 32rpx;
}

.machine_address {
  display: flex;
  align-items: flex-start;
  margin-top: 5rpx;
  vertical-align: top;
  width: 500rpx;
  font-size: 20rpx;
  color: #999999;
  margin-bottom: 18rpx;
}

.machine_address image {
  display: inline-block;
  height: 22rpx;
  width: 22rpx;
  margin-right: 8rpx;
  margin-top: 4rpx;
}

.machine_address view {
  display: inline-block;
  /* height: 35rpx; */
  font-size: 20rpx;
  width: 484rpx;
  line-height: 1rem;
}
.machine_navigation {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 30rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 15px;
  background-color: #082F40;
  border-radius: 4rpx;
  overflow: hidden;
  padding: 4rpx 5rpx 0rpx;
}

.machine_navigation image {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30rpx;
  height: 30rpx;
  margin-top: 5rpx;
}

.machine_navigation view {
  display: inline-block;
  height: 40rpx;
  margin-right: 8rpx;
  font-size: 28rpx;
}


.BigBoxbwt {
  width: 100%;
  height: 100%;
  overflow: visible;
  display: flex;
  align-items: center;
  justify-content: space-between;
}


.machine_detail_bottom {
  border-top: 1rpx solid #EEEEEE;
  height: 70rpx;
  width: 100%;
  /* padding-top: 20rpx; */
}

.machine_stock_con {
  height: 28rpx;
  display: inline-block;
  margin-left: 30rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.machine_stock {
  display: inline-block;
  font-size: 20rpx;
}

.red {
  display: inline-block;
  color: red;
  font-size: 27rpx;
  height: 28rpx;
  vertical-align: top;
}

.machine_feedback {
  display: inline-block;
  margin-right: 30rpx;
  /* float: right; */
}

.machine_feedback_title {
  width: 106rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-content: center;
  margin-right: 30rpx;

}

.machine_feedback_title cover-view {

  font-size: 20rpx;
  color: #999999;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 28rpx;
}

.cover-Image {

  width: 10rpx;
  height: 16rpx;
  margin-bottom: 9rpx;
}

.top .machine_detail .machine_detail_con .machine_detail_bottom .BigBoxbwt .erro {
  width: 164rpx;
  height: 28rpx;
  display: inline-block;
  margin-left: 30rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.machine_feedback_title view {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 11px;
  color: #999999;
}

.machine_feedback_title image {
  width: 10rpx;
  height: 16rpx;
  margin-top: 4rpx;
}

.bottom {
  width: 690rpx;
  height: 320rpx;
  margin: 36rpx auto 0;
}

.bottom image {
  width: 690rpx;
  height: 320rpx;
  border-radius: 16rpx;
  /* background-color: #141616; */
}