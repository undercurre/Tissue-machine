.tui-content-box{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 21%;
}

.logo {
  width: 190rpx;
  height: 190rpx;
  margin: 0 auto;
  /* border: 4rpx solid rgb(223,224,223); */
  overflow: hidden;
  border-radius: 10%;
}

.logo .image {
  width: 190rpx;
  height: 190rpx;
}

.name{
  font-size: 14px;
  font-weight: bold;
  margin: 40rpx auto 0;
  width: 300;
  text-align: center;
}

.version{
  font-size: 14px;
  font-weight: bold;
  margin: 10rpx auto 0;
  color: rgb(134,134,134);
  width: 200rpx;
  text-align: center;
}

.desc{
  position: relative;
  width: 600rpx;
  text-align: center;
  margin: 0 auto;
  font-weight: bold;
  font-size: 12px;
  color: rgb(157,157,157);
  top: 43%;
}