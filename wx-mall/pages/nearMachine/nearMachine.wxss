page {
  background-color: #EEEEEE;
}

.pageIndex {
  padding: 2rpx 0;
}

.machine_detail {
  background-color: white;
  border-radius: 10rpx;
  /* margin: 4%;
  padding: 25rpx; */
  margin: 30rpx;
  padding: 30rpx 20rpx 14rpx 28rpx;
}

.machine_detail_top {
  display: flex;
  border-bottom: 1rpx solid #f2f2f2;
  padding: 0 0 20rpx 0;


}

.middle {
  /* margin: 0 30rpx; */
  margin: 0 0 0 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 458.4rpx;
}

.machine_image {
  width: 160rpx;
  height: 160rpx;
  border-radius: 6rpx;
}


.machine_name {
  /* height: 52rpx; */
  /* width: 330rpx; */
  line-height: 1;
  font-size: 30rpx;
  font-weight: bold;
  color: #141616;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.machine_scan {
  width: 100rpx;
  height: 130rpx;
  margin-right: 30rpx;
  float: right;
  text-align: center;
  border-radius: 4rpx;
}

.machine_scan_image {
  width: 50rpx;
  height: 50rpx;
  margin: 20rpx auto;
}

.machine_scan_text {
  color: #141616;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 1;
  width: 110rpx;
  text-align: right;
  /* float: right;
  padding-right: 20rpx; */
  /* line-height: 52rpx; */

}

.machine_error {
  display: inline-block;
  width: 135rpx;
  height: 130rpx;
  text-align: center;
  font-size: 12px;
  line-height: 130rpx;
  margin-left: 10rpx;
  color: #FF0000;
}

.machine_detail_center {
  width: 100%;
  line-height: 40rpx;
}

.machine_distance {
  width: 120rpx;
  height: 130rpx;
  line-height: 130rpx;
  text-align: center;
  position: relative;

}

.machine_address {
  display: flex;
  align-items: center;
  color: #999999;
}

.red {
  display: inline-block;
  color: red;
  font-size: 24rpx;
  vertical-align: top;
}

.BigBoxbwt {
  width: 100%;
  height: 100%;
  overflow: visible;
  display: flex;
  align-items: center;
  margin-top: 4rpx;
}

.machine_stock_con {
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.machine_stock {
  display: inline-block;
  font-size: 24rpx;
  height: 100%;
  vertical-align: top;
}

.machine_detail_bottom {
  font-size: 24rpx;
  padding: 8rpx 0 0 0;
}

.text_size {
  font-size: 24rpx;
}

.subFrameTop {
  height: 34rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.icon {
  width: 22rpx;
  height: 22rpx;
  margin-right: 14rpx;
}

.addressDetail {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}