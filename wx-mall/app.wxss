/**app.wxss**/
pages {
  position: relative;
  z-index: 9999998;
  background: #fff;
  font-family: Arial, Helvetica, sans-serif;
}

.container {
  box-sizing: border-box;
  background-color: #f4f4f4;
  font-family: Arial, Helvetica, sans-serif;
}

view,
image,
text,
navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

view,
text {
  font-family: Arial, Helvetica, sans-serif;
  font-size: 29rpx;
}